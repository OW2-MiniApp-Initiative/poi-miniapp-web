<footer class="page-footer grey darken-3 z-depth-2">
  <div class="row">
    <div class="col s12 right-align">
      <a class='dropdown-trigger btn btn-flat' href='#' data-target='dropdown-lang'><i class="material-icons">language</i></a>

      <!-- Language Dropdown Structure -->
      <ul id='dropdown-lang' class='dropdown-content grey darken-3'>
        <% availableLanguages.forEach((lang) => { %>
          <li><a href="/town/<%= lang%>?url=<%= encodeURIComponent(url) %>" title="Relaunch the app in <%= i18n.t(`lang.${lang}`) %>">
            <%= i18n.t(`lang.${lang}`) %></a>
          </li>
        <% }) %>
      </ul>
      &nbsp; | &nbsp;  
      <a href="<%= content.app.issue_url %>" target="_blank"><%= i18n.t('index.feedback') %></a> 
      &nbsp; | &nbsp;
      <a class="modal-trigger" href="#modalabout"><%= i18n.t('index.about') %></a>
    </div>
  </div>
</footer>
